{
  "name": "trials_of_chaos",
  "private": true,
  "version": "1.0.0",
  "description": "Guide for the Trials of Chaos game mode in Path of Exile 2",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "serve -s dist -l 80",
    "start:prod": "nginx -c \"$PWD/nginx.conf\" -p \"$PWD\" -g 'daemon off;'",
    "dev": "vite --mode development",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "docker:start": "pnpm build && pnpm start:prod"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.34.33",
    "@tanstack/react-form": "^1.3.2",
    "@tanstack/react-query": "^5.72.2",
    "@tanstack/react-ranger": "^0.0.4",
    "@tanstack/react-router": "^1.116.0",
    "@tanstack/react-virtual": "^3.13.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^5.0.0",
    "vite-plugin-env-compatible": "^2.0.1"
  },
  "devDependencies": {
    "@tailwindcss/container-queries": "^0.1.1",
    "@tanstack/react-router-devtools": "^1.116.0",
    "@tanstack/router-vite-plugin": "^1.20.2",
    "@types/node": "^22.14.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.21",
    "dotenv": "^16.5.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "serve": "^14.2.4",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
